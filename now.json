{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next@canary"
    },
    {
      "src": "static/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "^/(.*)[.](html|ico|jpg|json|pdf|png|svg|txt|webp|xml)$",
      "dest": "/static/$1.$2"
    }
  ],
  "regions": ["all"]
}
